#include <physical_layouts.dtsi>
#include <nrfTest.overlay>

/ {
    physical_layout0: physical_layout_0 {
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";
        transform = <&default_kscan>;
        kscan = <&default_transform>;

        keys  //                     w   h x y rot rx ry
            = <&key_physical_attrs 100 100 0 0   0  0  0>, <&key_physical_attrs 100 100 100 0   0  0  0>,
              <&key_physical_attrs 100 100 0 100 0  0  0>, <&key_physical_attrs 100 100 100 100 0  0  0>;
    };
};